script/source = "extends Control

# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"

# Called when the node enters the scene tree for the first time.
func _ready():

	var skip_first = true
	for child in get_children():
		if skip_first :
			skip_first = false
			continue
		child.modulate = Color(1,1,1,0)

	pass # Replace with function body.

var timer          = 0.0
var timer_rest     = 2.5
var children_index = 1
var increase_flag = true

func _process(delta):
	
	if Input.is_action_just_released(\"ui_accept\"): 
		get_tree().change_scene(\"res://Scenes/Main.tscn\")
		
	if children_index == get_child_count()-1 and !increase_flag : 
		#get_tree().change_scene(\"res://Scenes/Main.tscn\")
		return
	
	if increase_flag: timer += delta
	else: timer -= delta
	
	var active_child = get_children()[children_index]
	
	timer = min( timer, timer_rest )
	timer = max( timer,          0 )
	
	var scale =  timer/timer_rest
	
	active_child.modulate = Color( 1,1,1, scale )
	
	if timer == timer_rest: increase_flag = false
	if timer == 0         : 
		increase_flag = true
		children_index += 1
"

